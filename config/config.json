{
  "log": {
    "logger_manager": {
      "enable_files_logs_monitoring_only_for_one_logger": true,
      "enable_dynamic_config_update": true,
      "enable_unique_logger_identifier": false
    },
    "logger": {
      "colors": "ClassicColors",
      "path": "logs",
      "decorator_log_level": "DEBUG",
      "print_log_level": "DEBUG",
      "file_log_level": "DEBUG",
      "print_log": true,
      "write_to_file": false,
      "display_monitoring": false,
      "files_monitoring": false,
      "file_size_unit": "Go",
      "disk_alert_threshold_percent": 0.8,
      "log_files_size_alert_threshold_percent": 0.2,
      "max_log_file_size": 1.0,
      "identifier_max_width": 15,
      "filename_lineno_max_width": 15
    }
  },
  "gliner": {
    "model_name": "gliner-community/gliner_medium-v2.5",
    "context_window": 10,
    "threshold": 0.5
  },
  "display": {
    "window_size": 20
  }
}